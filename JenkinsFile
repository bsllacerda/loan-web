pipeline {
  agent {
    node {
        label 'nodejs' 
    }
  }
  options {
    timeout(time: 20, unit: 'MINUTES') 
  }
  stages {
    stage('loadpipeline') {
        node {
            apply from: 'https://raw.githubusercontent.com/bsllacerda/openshift/dev/build-configs/pipelines/nodejs-pipeline.groovy'
        }
    }
  }   
}